{
  "name": "hr_management_app",
  "version": "1.0.0",
  "description": "HR Management App",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "tsc": "tsc",
    "jasmine": "jasmine",
    "test": "npm run build && npm run jasmine",
    "dev": "nodemon src/server.ts",
    "start": "ts-node src/server.ts",
    "prod": "node build/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess \"node ./build/server.js\"",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down-all": "npx sequelize-cli db:migrate:undo:all"
  },
  "author": "Abdeladl Mounir Shaheen",
  "license": "NONE",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.0.0",
    "@types/sequelize": "^4.28.13",
    "@types/supertest": "^2.0.12",
    "@types/typescript": "^2.0.0",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.18",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
